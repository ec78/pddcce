    local fmt1;

   __print_mg_header(mgO);
   fmt1 = "%-20s%15s%5s%20s%15d";
    
   sprintf("%15s", "Variable"$~"Coef."$~"SE(NP)"$~"T-value(NP)"$~"P-val");
